<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <script src="@Url.Content("~/Scripts/jquery/jquery-1.7.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery/jquery-ui-1.8.13.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery/jquery.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap-dropdown.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap-collapse.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap-datepicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Utilidades.js")" type="text/javascript"></script>

      <link href="@Url.Content("~/Content/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/bootstrap-responsive.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/datepicker.css")" rel="stylesheet" type="text/css" />    
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />



    <link rel="SHORTCUT ICON" href="@Url.Content("~/Content/favicon.ico")" />
<script type="text/javascript">

    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-34367661-1']);
    _gaq.push(['_trackPageview']);

    (function () {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();

</script>
</head>




<body>
    <div class="page">
        <header>

        <div>
        <br />
            </div>


  <div id="navbar-example" class="navbar navbar-static">
            <div class="navbar-inner">
              <div class="container" style="width: auto;">
                <a class="brand" href="#">AM Mobile</a>
                <ul class="nav">
                  <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Ruta Courier<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                      <li>@Html.ActionLink("Listar", "Index", "Ruta")</li>
                      <li>@Html.ActionLink("Crear", "Create", "Ruta")</li>
                      <li>@Html.ActionLink("Cargar Rutas", "Upload", "Ruta")</li>

                    </ul>
                  </li>

                  <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Reporte<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                       <li>@Html.ActionLink("Reporte OE", "ReporteOE", "Reportes")</li>
                    </ul>
                  </li>




                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Parametrización<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                       <li>@Html.ActionLink("Usuarios", "Index", "Usuario")</li>
    
                    </ul>
                  </li>



                 
                </ul>
                <ul class="nav pull-right">
                 <li>@Html.ActionLink("Salir", "LogOff", "Account")</li>
                </ul>
              </div>
            </div>
          </div> <!-- /navbar-example -->

            
        </header>

        <section id="main">
            @RenderBody()
        </section>


        <footer>

        Copyright © 2013 - CGG.  Versión 1.0.1 <br>
        All Rights Reserved.
        </footer>
    </div>
</body>
</html>
