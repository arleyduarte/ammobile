<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <script src="@Url.Content("~/Scripts/jquery/jquery-1.7.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery/jquery-ui-1.8.13.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery/jquery.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap-dropdown.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap-collapse.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap/bootstrap-datepicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Utilidades.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/Content/themes/south-street/jquery-ui-1.8.13.custom.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/bootstrap-responsive.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/datepicker.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Buttons.css")" rel="stylesheet" type="text/css" />
    <link rel="SHORTCUT ICON" href="@Url.Content("~/Content/favicon.ico")" />
</head>
<body>
    <div class="page">
        <header>

                    <div id="logindisplay">
                @Html.Partial("_LogOnPartial")
            </div>


  <div id="navbar-example" class="navbar navbar-static">
            <div class="navbar-inner">
              <div class="container" style="width: auto;">
                <a class="brand" href="#">Wasser Chemical</a>
                <ul class="nav">
                  <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Clientes<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                      <li>@Html.ActionLink("Listar Miembros", "Index", "Member")</li>
                      <li>@Html.ActionLink("Crear Miembro", "Create", "Member")</li>
                       <li class="divider"></li>
                     <li>@Html.ActionLink("Buscar Miembro", "GetListName", "Member")</li>
                    </ul>
                  </li>

                  <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Farmacias<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                       <li>@Html.ActionLink("Listar Ministerios", "Index", "Ministry")</li>
                      <li>@Html.ActionLink("Crear Ministerio", "Create", "Ministry")</li>

                    </ul>
                  </li>

                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Médicos<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                       <li>@Html.ActionLink("Listar Médicos", "Index", "Medico")</li>
                      <li>@Html.ActionLink("Crear Médico", "Create", "Medico")</li>

                    </ul>
                  </li>

                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Rutas<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                       <li>@Html.ActionLink("Reporte Visitas", "Index", "Medico")</li>
                      <li>@Html.ActionLink("Crear Médico", "Create", "Medico")</li>

                    </ul>
                  </li>

                  <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Certificados<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                      <li> <a href="#">Buscar Certificado</a></li>
                      <li class="divider"></li>
                      <li class="nav-header">Formatos</li>

                    </ul>
                  </li>






                  <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Informes<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                      <li class="divider"></li>
                      <li class="nav-header">Operativos</li>
                      <li>@Html.ActionLink("Crecimiento Iglesia", "MembersByMonth", "Reports")</li>

                       <li class="nav-header">Indicadores</li>
                      <li>@Html.ActionLink("Miembros por Ministerio", "MembersByMinistry", "Reports")</li>

                    </ul>
                  </li>

                  <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Parametrización<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                      <li class="divider"></li>



                       <li class="divider"></li>
                       <li class="nav-header">Generales</li>

                      <li><a href="#">Cambiar Clave</a></li>


                    </ul>
                  </li>
                 
                </ul>
                <ul class="nav pull-right">
                 <li>@Html.ActionLink("Salir", "LogOff", "Account")</li>
                </ul>
              </div>
            </div>
          </div> <!-- /navbar-example -->

            
        </header>
        <section id="main">
            @RenderBody()
        </section>
        <footer>

        Copyright © 2012 - CGG. Versión 1.0.0<br>
        All Rights Reserved.
        </footer>
    </div>
</body>
</html>
